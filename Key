/**
 * 
 * 
 * @author (your name) 
 * @version (a version number or a date)
 */
public class Key

{
    private boolean foundKey = false;
    private Chest onChest;
    
    public Key()
    {
      this.foundKey = false;
    }

    public void use(Chest onChest)
    {
        onChest.unLock(this);
    }

    public boolean getKey()
    {
        return foundKey;
    }
    
    public void findKey()
    {
         this.foundKey = true;
    }
}
