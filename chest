
/**
 * This is the chest class which holds an object that must be unlocked in one of the rooms.
 */
public class Chest
{
    // instance variables - replace the example below with your own
    private boolean chestIsLocked = true;
    private Key key;
    //private Key key;

    public void unLock(Key theKey) 
    {
        if(key == theKey)
        {
            chestIsLocked = false; 
        }
    }

    /**
     * Should return true if the chest is locked, false otherwise
     */
    public boolean isLocked()
    {
        return chestIsLocked;
    }

}
